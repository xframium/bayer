<?xml version="1.0" encoding="UTF-8"?>
<validationCollection xsi:schemaLocation="http://www.xframium.org/structureValidation http://www.xframium.org/xsd/validation/structureValidation-0.0.1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.xframium.org/structureValidation">
    
     <validation name="H1Tag">
	    <description>H1 Text Verification</description>
	   		<tag name="//h1/span"> 
	    	<text type="contains">Cat Education</text>
	    	</tag>
    </validation>
    <validation name="H2Tag">
    <description>H2 Text Verification</description>
   		<tag name="//h2/span"> 
	    	<text type="contains">Flea bites can hurt your cat — and your cat’s health</text>
	    	<text type="contains">Save on Bayer products today.</text>
	    	<text type="contains">Looking for Bayer products?</text>
	    	<text type="contains">The best is</text>
    	</tag>
    	<tag name="//p"> 
    	<text type="contains">Seeing a flea on your clean, well-cared for cat can be a horrifying experience.</text>
    	</tag>
    </validation>
    
    
</validationCollection>